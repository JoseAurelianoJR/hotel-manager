<h1>Listing rooms</h1>
 
<table>
    <tr>
        <th>Number</th>
        <th>Available</th>
    </tr>
 
      <% @rooms.each do |room| %>
            <tr>
            <td><%= room.number %></td>
            <td><%= room.available %></td>
            <td><%= link_to 'Show', room_path(room) %></td>
            <td><%= link_to 'Edit', edit_room_path(room) %></td>
            <td><%= link_to 'Delete', room_path(room), method: :delete,
        data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
</table>

<br><%= link_to 'New room', new_room_path %>
<br><%= link_to 'Back', welcome_index_path %>